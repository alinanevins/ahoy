<div class="container">
  <h1>New Classes*v2 to Canvas Transition</h1>

  <%= form_for(@transition) do |f| %>

  <%= f.label :user_id %>
  <%= f.text_field :user_id %><p/>

  <%= f.label :faculty_id %>
  <%= f.text_field :faculty_id %><p/>

  <%= f.label :course %>
  <%= f.text_field :course %><p/>

